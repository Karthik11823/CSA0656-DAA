#include <stdio.h>
int fibonacci(int n) {
    if (n <= 0) return 0; 
    if (n == 1) return 1;   
    return fibonacci(n - 1) + fibonacci(n - 2);
}
void print_fibonacci_series(int n) {
    printf("Fibonacci Series:\n");
    for (int i = 0; i < n; ++i) {
        printf("%d ", fibonacci(i));
    }
    printf("\n");
}
int main() {
    printf("Name: B. karthik\n");
    printf("Regno: 192211570\n");
    int num_terms = 10; 
    print_fibonacci_series(num_terms);
    return 0;
}![Screenshot 2024-07-18 130551](https://github.com/user-attachments/assets/a8f1823f-16bf-472a-a532-bcd844ead56c)
