#include <stdio.h>

// Name: B.Karthik
// Register Number: 192211570

void printPascal(int n) {
    for (int line = 1; line <= n; line++) {
        int C = 1;
        for (int i = 1; i <= line; i++) {
            printf("%d ", C);
            C = C * (line - i) / i;
        }
        printf("\n");
    }
}

int main() {
    char name[] = "B.Karthik";
    char regno[] = "192211570";
    printf("Name : %s\n", name);
    printf("Regno : %s\n\n", regno);

    int n = 5;
    printPascal(n);

    return 0;
}
